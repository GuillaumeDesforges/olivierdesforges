{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch NextJS",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/next/dist/bin/next"
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome debug",
      "url": "http://localhost:3000"
    }
  ]
}
